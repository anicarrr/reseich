# Dynamic SDK Configuration
NEXT_PUBLIC_DYNAMIC_ENVIRONMENT_ID=your-dynamic-environment-id
NEXT_PUBLIC_WEBHOOK_URL=your-dynamic-environment-url

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-supabase-publishable-key

# n8n Workflow Integration (Single Webhook URL)
NEXT_PUBLIC_WEBHOOK_URL=your-single-n8n-webhook-url
# Alternative: N8N_WEBHOOK_URL=your-single-n8n-webhook-url
# Form IDs for different workflows:
# - research: triggers research workflow
# - chat: triggers AI chat workflow  
# - email: triggers email delivery workflow

# Email Service (SendGrid recommended)
SENDGRID_API_KEY=your-sendgrid-api-key
EMAIL_FROM=noreply@reseich.com
EMAIL_FROM_NAME=ReSeich Platform

# Sei Network EVM Configuration
# Mainnet
#SEI_RPC_URL=https://evm-rpc.sei-apis.com
#SEI_CHAIN_ID=1329

# Testnet
SEI_RPC_URL=https://evm-rpc-testnet.sei-apis.com
SEI_CHAIN_ID=1328
SEI_EXPLORER_URL=https://seitrace.com/
SEI_CURRENCY_SYMBOL=SEI
SEI_DECIMALS=18

# Force testnet usage (optional - defaults to testnet in development)
NEXT_PUBLIC_USE_TESTNET=true

# File Storage
SUPABASE_STORAGE_BUCKET=research-files
SUPABASE_STORAGE_URL=https://yvmtsalztdymstpvgpko.storage.supabase.co/storage/v1/s3

# Security & Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

NEXT_PUBLIC_USE_WEBHOOK_TOOL=false
